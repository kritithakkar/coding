import java.util.*;
import java.lang.*;
import java.io.*
public class Main
{
public int ele(int arr[])
{
int res=arr[0];
int l=arr.length;
for(int i=1;i<l;i++)
{
res=res^arr[i];
}
return res;
}
public static void main(string args[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int arr[]=new int[n];
for(int i=0;i<n;i++)
{
arr[i]=sc.nextInt();
}
System.out.println(ele(arr));
}
}
// XOR of a number with itself is 0.
// XOR of a number with 0 is number itself.
