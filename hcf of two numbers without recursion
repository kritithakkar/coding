import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		if(a==0)
		System.out.print(b);
		if(b==0)
		System.out.print(a);
		while(a!=b)
		{
		    if(a>b)
		    {
		        a-=b;
		    }
		    else{
		        b-=a;
		    }
		}
		System.out.print(a);
	}
}
